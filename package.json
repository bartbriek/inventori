    ]
  },
  "dependencies": {
    "axios": "1.4.0",
    "body-parser": "1.20.2",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "aws-sdk": "2.1430.0",
    "cors": "2.8.5",
    "@mui/material": "^5.14.4",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "prettier": "3.0.1",
    "nodemon": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "backend": "nodemon server/src/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
